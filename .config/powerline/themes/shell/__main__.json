{
  "segment_data": {
    "branch": {
      "before": " "
    },
    "gitstatus": {
      "args": {
        "formats": {
          "branch": "\ue0a0 {}",
          "tag": " ★ {}",
          "behind": " ↓ {}",
          "ahead": " ↑ {}",
          "staged": " ● {}",
          "unmerged": " ✖ {}",
          "changed": " ✚ {}",
          "untracked": " … {}",
          "stashed": " ⚑ {}"
        }
      }
    },
    "hostname": {
      "args": {
        "only_if_ssh": true
      }
    },
    "cwd": {
      "args": {
        "dir_limit_depth": 3
      }
    }
  }
}
